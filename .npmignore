# Ignore node_modules folder
node_modules/

# Ignore build output from local development
dist/

# Ignore any logs
*.log

# Ignore environment variable files
.env
.env.*

# Ignore local configuration files
*.local

# Ignore testing files and directories
coverage/
*.test.js
*.spec.js

# Ignore documentation and static files used only for local development
docs/
public/

# Ignore IDE and editor configuration files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore Webpack configuration file if not needed in the package
webpack.config.js

# Ignore Babel configuration file if not needed in the package
.babelrc
babel.config.js

# Ignore ESLint configuration files
.eslintignore
.eslintrc.js
.eslintrc.json
.eslintrc.yaml

# Ignore Prettier configuration files
.prettierignore
.prettierrc
.prettierrc.js
.prettierrc.json
.prettierrc.yaml
.prettierrc.toml

# Ignore npm debug files
npm-debug.log*

# Ignore Yarn lock file (optional, if using npm)
yarn.lock

# Ignore other miscellaneous files
.editorconfig
.gitattributes
